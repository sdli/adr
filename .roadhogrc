{
  "entry": "src/index.js",
  "disableCSSModules": false,
  "publicPath": "/",
  "autoprefixer": null,
  "extraBabelPlugins": [
    "transform-runtime",
    ["import", { "libraryName": "antd", "style": true }]
  ],
  "theme": {
    "@primary-color": "#108EE9",
    "@link-color": "#108EE9",
    "@border-radius-base": "4px",
    "@font-size-base": "14px",
    "@header-background-color": "#000",
    "@border-sharp": "1px solid #f0f0f0"
  },
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:3060/",
      "changeOrigin": false,
      "pathRewrite": { "^/api" : "" }
    }
  }
}
